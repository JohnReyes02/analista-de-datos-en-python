     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Set the context to \"paper\"\n",
    "sns.set_context('paper')\n",
    "\n",
    "# Create bar plot\n",
    "sns.catplot(x='Siblings', y='Loneliness',\n",
    "            data=survey_data, \n",
    "            kind='bar', \n",
    "            hue='Siblings', legend=False)\n",
    "\n",
    "# Show plot\n",
    "plt.show()"
   ]
  },
